pipeline {
agent {
  docker { 
            image 'sravangcpdocker/ansible-gcp:1'
            args '-u root:root'
          }
         }
  stages {
    stage('Hello') {
      steps {
        sh '''
          ansible --version
          git --version
          ansible-playbook --version
          ansible-galaxy --version
          git clone https://github.com/sravan-github/jenkins-example-ansible.git
        '''
      }
    }
  }
}
